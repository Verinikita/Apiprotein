{
    "version": 2,
    "builds": [
      { "src": "app.py", "use": "@vercel/python" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/app.py" }
    ]
  }
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@now/python"
    }
  ],
  "routes": [
    { "src": "/upload", "dest": "/app.py" },
    { "src": "/.*", "dest": "/app.py" }
  ]
}
